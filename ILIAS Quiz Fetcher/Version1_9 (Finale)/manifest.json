{
  "name": "Test Fetcher",
  "version": "1.9",
  "description": "Fetches HTML code and autofills your test with a solution.html. Author: me",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs", "notifications", "http://*/", "https://*/", "downloads"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },

  "manifest_version": 2
}
